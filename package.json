{
  "name": "VAST-Tester",
  "version": "0.0.0",
  "description": "Tests IAB VAST ads.",
  "private": true,
  "homepage": "https://vasttester.iabtechlab.com/",
  "repository": "InteractiveAdvertisingBureau/VAST-Tester",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12",
    "yarn": ">=1.7.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf build",
    "build": "yarn run clean && REACT_APP_BUILD_DATE=\"$( TZ=GMT date )\" react-scripts build",
    "buildwin": "yarn run clean && react-scripts build",
    "test": "standard \"src/**/*.js\"",
    "format": "prettier-standard \"src/**/*.js\" && standard --fix \"src/**/*.js\""
  },
  "dependencies": {
    "base16": "^1.0.0",
    "debug": "^4.1.1",
    "fecha": "^4.2.0",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "husky": "^4.2.5",
    "iab-vast-loader": "^2.2.0",
    "lint-staged": "^10.2.2",
    "lodash-es": "^4.17.11",
    "lower-case-first": "^2.0.1",
    "node-sass": "^4.12.0",
    "p-defer": "^3.0.0",
    "p-timeout": "^3.1.0",
    "prettier-standard": "^16.3.0",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-collapsible": "^2.6.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-fontawesome": "^1.6.1",
    "react-json-tree": "^0.11.2",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.4.1",
    "react-tabs": "^3.0.0",
    "react-textarea-autosize": "^7.1.0",
    "react-toggle": "^4.0.2",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.1.0",
    "reset-css": "^5.0.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.2",
    "serialize-error": "^7.0.1",
    "standard": "^14.3.4",
    "upper-case-first": "^2.0.1",
    "url-search-params": "^1.1.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-standard",
      "standard --fix"
    ]
  },
  "standard": {
    "globals": [
      "Blob"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
